{
  "name": "oclif-template",
  "version": "0.0.1",
  "description": "Freeze your dependencies in `package.json`.",
  "author": "Moch. Nafkhan Alzamzami @nafkhanzam",
  "bin": {
    "oex": "./bin/run.js"
  },
  "homepage": "https://github.com/nafkhanzam/oclif-template",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "nafkhanzam/oclif-template",
  "bugs": "https://github.com/nafkhanzam/oclif-template/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "type": "module",
  "oclif": {
    "bin": "oex",
    "dirname": "oex",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "test": "jest"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@oclif/core": "2.1.2",
    "@oclif/plugin-help": "5.2.4"
  },
  "devDependencies": {
    "@nafkhanzam/nz-cli": "0.0.29",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.34.0",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "globby": "^11.1.0",
    "jest": "29.4.2",
    "nyc": "15.1.0",
    "oclif": "3.6.3",
    "prettier": "2.8.4",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tsconfig-paths-jest": "0.0.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  }
}
